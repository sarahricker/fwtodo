{
  "name": "todomvc-vue-composition-api",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blacksonic/todomvc-vue-composition-api.git"
  },
  "scripts": {
    "start": "vue-cli-service serve",
    "format": "prettier --write 'src/**/*.{js,vue}'",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix",
    "report": "vue-cli-service build --report",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vue-cli-service test:unit --recursive 'src/**/*.spec.js'",
    "test:e2e": "vue-cli-service test:e2e --headless"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "uuid": "^8.3.0",
    "vue": "^3.0.0",
    "vuex": "^4.0.0-beta.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-e2e-cypress": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-plugin-unit-mocha": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "2.0.0-alpha.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.1.2",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "vue-loader": "^16.0.0-beta.7"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 firefox version"
  ]
}
